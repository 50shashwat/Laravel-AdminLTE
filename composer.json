{
  "name": "kalpvaig/laravel-adminlte",
  "description": "Easy AdminLTE integration with Laravel",
  "keywords": ["laravel", "adminlte", "admin", "administrator"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jeroen Noten",
      "email": "jeroennoten@me.com"
    },
    {
      "name": "Shashwat Singh",
      "email": "parihar.shashwat@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Kalpvaig\\LaravelAdminLte\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/Console/CommandTestCase.php"
    ]
  },
  "extra": {
      "laravel": {
          "providers": [
              "Kalpvaig\\LaravelAdminLte\\AdminLteServiceProvider"
          ]
      }
  },
  "require": {
    "laravel/framework": ">=6.0",
    "php": ">=7.2.0",
    "almasaeed2010/adminlte": "3.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.1",
    "orchestra/testbench": ">=4.0",
    "scrutinizer/ocular": "^1.9"
  }
}
